generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Goal {
  id         String   @id @default(cuid())
  original   String
  complexity Int
  steps      Json
  createdAt  DateTime @default(now())

  @@index([createdAt])
}
