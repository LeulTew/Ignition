GEMINI_API_KEY=your_gemini_api_key_here
DATABASE_URL="postgresql://user:password@host:5432/database_name?sslmode=require&channel_binding=require"
# uncomment next line if you use Prisma <5.10
# DATABASE_URL_UNPOOLED="postgresql://user:password@host:5432/database_name?sslmode=require&channel_binding=require"
GEMINI_MODEL_CHAIN=gemini-2.5-flash,gemini-2.5-flash-lite,gemini-2.0-flash
GEMINI_TEMPERATURE=0.1
GEMINI_MAX_OUTPUT_TOKENS=512
GEMINI_MAX_RETRIES=3
GEMINI_RETRY_BASE_DELAY=1.0
